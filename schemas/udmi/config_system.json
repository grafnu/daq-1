{
  "title": "System Config snippet",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "max_update_ms": {
      "type": "number",
      "minimum": 10000
    },
    "min_loglevel": {
      "type": "number",
      "minimum": 100,
      "maximum": 800
    },
    "auth_key": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "private": {
          "type": "string"
        }
      },
      "required": [
        "private"
      ]
    }
  }
}
